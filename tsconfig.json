{
  "include": ["stu0"],
  "compilerOptions": {
    "target": "es2016",              
    "module": "commonjs",                                    
    "sourceMap": true,                                               
    "outDir": "./dist",                                
    "removeComments": true,                         
    "esModuleInterop": true,           
    "forceConsistentCasingInFileNames": true,      
    "strict": true,                                   
    "noUnusedParameters": true,                           
    "noImplicitReturns": true,        
    "skipLibCheck": true                          
  }
}
